{
  "family": "my-ecs-service",               
  "containerDefinitions": [
    {
      "name": "my-container",                
      "image": "REPOSITORY_URI:TAG",         
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}